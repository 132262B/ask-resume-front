{
  "name": "ask-resume-front",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "pretty": "prettier --write \"src/**/*.(ts|tsx)\"",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "deploy": "turbo run build --scope=app --includeDependencies --no-deps",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "test": "jest",
    "test:shared-lib": "jest --projects packages/shared-lib"
  },
  "turboConfig": "./turbo.json",
  "workspace": [
    "app",
    "packages/*"
  ],
  "keywords": [],
  "author": "dev-redo",
  "license": "ISC",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "turbo": "^1.8.8",
    "typescript": "^5.0.3"
  },
  "lint-staged": {
    "{app, packages/***}/**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
